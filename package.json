{
  "name": "automerge-dependabot",
  "version": "1.0.0",
  "description": "GitHub Action to automatically merge Dependabot pull requests",
  "main": "dist/index.js",
  "bin": {
    "automerge-dependabot": "bin/automerge-dependabot"
  },
  "scripts": {
    "build": "ncc build src/index.js -o dist --minify",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "prepare": "husky",
    "cli": "node bin/automerge-dependabot"
  },
  "keywords": [
    "github",
    "actions",
    "dependabot",
    "automerge"
  ],
  "author": "navikt",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.2",
    "@actions/github": "^8.0.0",
    "commander": "^14.0.2",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jest": "^30.2.0"
  }
}
